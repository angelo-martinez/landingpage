transform: uppercase;\n            @media (min-width: 768px) {\n                font-size: 6rem;\n            }\n        }\n        h2 {\n            font-size: 1.4rem;\n            @media (min-width: 768px) {\n                font-size: 4rem;\n            }\n        }\n        p {\n            font-size: 1.6rem;\n            @media (min-width: 768px) {\n                font-size: 1.4rem;\n            }\n        }\n    }\n\n    &-gradient {\n        position: absolute;\n        bottom: 0;\n        left: 0;\n        width: 100%;\n        height: 0;\n        background-image: linear-gradient(transparent 15%, $color-milk);\n        @media (min-width: 768px) {\n            height: 10vh;\n        }\n    }\n}\n\n.intro {\n    padding: 5rem 1.5rem;\n    max-width: 94rem;\n    margin: 0 auto;\n    display: block;\n    color: $color-grey;\n\n    h1 {\n        text-align: center;\n        font-weight: 600;\n        text-transform: uppercase;\n        font-size: 2.8rem;\n        line-height: 1.21;\n        margin-bottom: 1rem;\n    }\n\n    p {\n        font-weight: 400;\n        font-size: 1.6rem;\n        line-height: 1.75;\n        margin-bottom: 10rem;\n    }\n\n    .winners-list {\n        box-shadow: 0 0.3rem 1rem 0.2rem rgba(5,5,5,.09);\n        padding: 10rem 3rem 7rem;\n\n        .winners-list-header {\n            max-width: 55.5rem;\n            height: 9rem;\n            margin: -15rem auto 0;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            text-align: center;\n            color: #fff;\n            background-color: $color-grey;\n        }\n\n        h3 {\n            font-size: 2.8rem;\n            font-weight: 300;\n            text-transform: uppercase;\n            @media (min-width: 768px) {\n                font-size: 3.8rem;\n            }\n        }\n\n        .container {\n            width: 20rem;\n            margin: 0 auto;\n            @media (min-width: 768px) {\n                width: 80%;\n            }\n        }\n\n        ul {\n            column-count: 1;\n            margin-top: 5rem;\n            font-size: 2rem;\n            list-style: none;\n            @media (min-width: 768px) {\n                column-count: 2;\n                padding-inline-start: 7rem;\n            }\n        }\n\n        li::before {\n            content: \"\\2022\";\n            color: $color-grey; \n            font-weight: bold;\n            display: inline-block; \n            width: 1rem; \n            margin-left: -1em;\n            margin-right: 1rem;\n        }\n    }\n}\n\n.grid {\n    padding: 0 1.5rem;\n    max-width: 94rem;\n    margin: 0 auto;\n    display: block;\n    .row {\n        display: flex;\n        flex-wrap: wrap;\n    }\n      \n    .column {\n        flex: 100%;\n        max-width: 100%;\n        padding: 0 .4rem;\n        @media (min-width: 768px) {\n            flex: 50%;\n            max-width: 50%;\n        }\n        @media (min-width: 1200px) {\n            flex: 25%;\n            max-width: 25%;\n        }\n\n        img {\n            margin-top: .8rem;\n            vertical-align: middle;\n            width: 100%;\n        }\n    }\n}\n\n#overlay {\n    position: fixed; \n    display: none; \n    width: 100%; \n    height: 100%; \n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    background-color: rgba(0,0,0,0.8); \n    z-index: 10; \n    cursor: pointer;\n    .overlay-box {\n        text-align: center;\n    }\n    h4 {\n        position: absolute;\n        top: 50%;\n        left: 50%;\n        font-size: 3rem;\n        text-transform: uppercase;\n        color: white;\n        transform: translate(-50%,-50%);\n        @media (min-width: 768px) {\n            font-size: 5rem;\n            white-space: nowrap;\n        }\n        @media (min-width: 1200px) {\n            font-size: 7rem;\n        }\n    }\n}\n\n.form {\n    padding: 5rem 1.5rem;\n    margin-top: 5rem;\n    display: block;\n    background-color: #F5F5F5;\n    text-align: center;\n\n    h5 {\n        font-size: 3rem;\n        text-transform: uppercase;\n        line-height: 1;\n        margin-bottom: 2rem;\n    }\n\n    .input-wrapper {\n        display: block;\n        margin: 1rem 0;\n        font-size: 1.6rem;\n    }\n\n    button {\n        font-size: 1.6rem;\n        border: 1px solid black;\n        background: white;\n        padding: .5rem 2rem;\n        margin-top: 2rem;\n    }\n}"},"lineCount":null}},"error":null,"hash":"47d6040f3acfad8a890e19afed0f0ae0","cacheData":{"env":{}}}